    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-plugin-px2rem": "^0.8.1",
    "prettier": "2.7.1",
    "release-it": "^15.1.1",
